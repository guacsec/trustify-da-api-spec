{
	"include": ["model/v3/**/*","model/v4/**/*","./package.json"],
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": "src",
		"declaration": true,
		"esModuleInterop": true,
		"lib": ["ES2020"],
		"types": ["node"],
		"moduleResolution": "Node",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"rootDir": ".",
		"skipLibCheck": true,
		"module": "esnext",
		"outDir": "dist",
		"target": "es2020",
		"resolveJsonModule": true,
		"strict": true
	}
}