{
	"include": ["model/**/*"],
	"exclude": ["node_modules", "dist"],
	"compilerOptions": {
		"target": "es2018",
		"module": "commonjs",
		"lib": ["es2018", "dom"],
		"declaration": true,
		"outDir": "dist",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false
	}
}